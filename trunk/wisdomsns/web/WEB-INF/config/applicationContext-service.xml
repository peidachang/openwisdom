<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
       http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd"
	default-autowire="byName">

	<bean id="userService" class="com.iwisdom.common.service.impl.UserServiceImpl"/>
	<bean id="menuService" class="com.iwisdom.common.service.impl.MenuServiceImpl"/>
	<bean id="roleService" class="com.iwisdom.common.service.impl.RoleServiceImpl"/>
	<bean id="groupService" class="com.iwisdom.common.service.impl.GroupServiceImpl"/>
	
	<bean id="utilService" class="com.iwisdom.common.service.impl.UtilServiceImpl"/>
	<bean id="contentManagementService" class="com.iwisdom.cms.service.impl.ContentManagementServiceImpl"/>
	<bean id="brandService" class="com.iwisdom.brand.service.impl.BrandServiceImpl"/>
	<bean id="serviceService" class="com.iwisdom.servicebus.service.impl.ServiceServiceImpl"/>
	
</beans>
